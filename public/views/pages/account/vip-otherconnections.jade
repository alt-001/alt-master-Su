include partials/vip-top
.col-xs-3
  include partials/vip-sidebar-other-connected
.col-xs-9
  #vipMyConnectionsContent
    .top-bar
      .col-xs-8.top-bar-header
        | My Connections
      .col-xs-4
        form(ng-model="vipSearch")
          .col-xs-8.pull-right
            input.search(type='text' placeholder='Search').col-xs-10
            button(type="submit").addon.col-xs-2
              span.glyphicon.glyphicon-search
    .connections
      .col-xs-4
        .media
          a.pull-left(href='#')
            img.media-object(ng-src='/assets/images/pro/brand.jpg')
          .media-body
            h4.media-heading Su Liang
            | Designer
      .col-xs-4
        .media
          a.pull-left(href='#')
            img.media-object(ng-src='/assets/images/pro/blogger.jpg')
          .media-body
            h4.media-heading Su Liang
            | Designer

      .col-xs-4
        .media
          a.pull-left(href='#')
            img.media-object(ng-src='/assets/images/pro/celebrity.jpg')
          .media-body
            h4.media-heading Su Liang
            | Designer
    .clearfix
    ul.pager
      li.previous.disabled
        a(href='#') Previous
      li.next
        a(href='#') Next
